package classes;

public class Conta{
    
    

	public Conta(int codigo, String nome, double valor, double limite) {
		super();
		this.codigo = codigo;
		this.nome = nome;
		this.valor = valor;
		this.limite = limite;
	}
    
    private int codigo;
    private String nome;
    private double valor;
    private double limite;
    
     public int getCodigo (){
        return this.codigo;
     }

     public void setCodigo (int codigo){
        this.codigo = codigo;
     }

    public String getNome (){
        return this.nome;
     }

     public void setNome (String nome){
         this.nome = nome;
      }

	public double getValor() {
		return valor;
	}

	public void setValor(double valor) {
		this.valor = valor;
	}

	public double getLimite() {
		return limite;
	}

	public void setLimite(double limite) {
		this.limite = limite;
	}
	
//	static void Depositar(double saldoAnterior, double valorADepositar) {
//		double saldoAtual = saldoAnterior + valorADepositar;
//		
//	}
//    
    
}
